        <div>
            <img class="nav-menu-img" src="/images/graphic-3.png">
        </div>

        <div class="nav-menu text-center sidebar">
            <div class="sidebar-title">Sections</div>
            <hr class="sidebar-divider">
            <ul><li class="nav-item">
                    <a class="nav-link active" href="#description">
                      <i class="nav-icon far fa-list-alt"></i>Description
                    </a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" href="#gallery">
                      <i class="nav-icon far fa-images"></i>Photos
                    </a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" href="#events">
                      <i class="nav-icon fas fa-calendar-alt"></i>Calendar
                    </a>
                  </li>
                  <li class="nav-item">
                      <a class="nav-link" href="/">
                        <i class="nav-icon fas fa-home"></i>Maclubz Homepage
                      </a>
                    </li></ul>
                  <div class="sidebar-title">Connect with Us</div>
      
                  <hr class="sidebar-divider">      
                  <div class="social-item">
                      <a class="nav-link" href="#">
                          <i class="nav-icon fas fa-envelope"></i>Email
                      </a>
                  </div>
      
                  <div class="social-item">
                      <a class="nav-link" href="#">
                        <i class="nav-icon fab fa-twitter-square"></i>Twitter
                      </a>
                  </div>
                  <div class="social-item">
                      <a class="nav-link" href="#">
                        <i class="nav-icon fab fa-facebook"></i>Facebook
                      </a>
                  </div>
        </div>

        <main class="container-fluid align-self-start">   
            <div class="row section section-club-highlight">
            <div class="col-md-10">
                <div>
                    <h1 class="display-5 section-title"><%= team.name %></h1>
                    <hr class="section-title-divider">
                </div>
                <div class="row content">

                    <div class="col-md-1"></div>
                    <div class="col-md-4 text-center"><img class="banner" src="/images/small_SEC.png"></div>
                    <div class="col-md-1"></div>
                    <div class="col-md-5 text-left club-quotes">
                        "Software design is for everyone, young and old, male and female"
                        <div class="quote-author">- Jayanthi Uthayakumar, Club President</div>
                        "The Software Engineering Society is full of talent, technology, and learning"
                        <div class="quote-author">- John Smith, Member, 1st Year Engineering Student</div>
                    </div> <!--End club-quotes-->
                </div>
            </div>
        </div> <!--End club-highlight-->

        <div class="row section section-club-description" id="description">
            <div class="col-md-10">
                <div>
                    <h1 class="display-5 section-title">Description</h1>
                    <hr class="section-title-divider">
                </div>
                <div class="row content">
                    <div class="col-md-10 text-left">
                        <%= team.description %>
                    </div>
                </div>
            </div>
        </div> <!--End club-description-->

        <div class="row section section-gallery" id="gallery">

            <div class="col-md-10">
                <div>
                  <h1 class="display-5 section-title-white">Photos</h1>
                  <hr class="section-title-divider-gallery">
              </div>
              <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="/images/open_source_img.jpg">
                        <!--Image source: nochallenge.net-->
                    </div>

                    <div class="carousel-item">
                        <img class="d-block w-100" src="/images/ml-infographic.jpg">
                        <!--Image source: Intel Developer Zone-->
                    </div>

                  <!-- <div class="carousel-item">
                    <img class="d-block w-100" src="ses-header.png">
                </div> -->
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>

        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
            <!-- <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="indicator active"></li>
                <li data-target="#myCarousel" data-slide-to="1" class="indicator"></li>
                <li data-target="#myCarousel" data-slide-to="2" class="indicator"></li>
            </ol> -->
        </div>
    </div> <!--End gallery-->

    <div class="row section section-calendar" id="events">
        <div class="col-md-10">
            <div>
                <h1 class="display-5 section-title">Calendar of Events</h1>
                <hr class="section-title-divider">
            </div>
            <iframe id="calendar" src="https://calendar.google.com/calendar/embed?height=600&amp;wkst=1&amp;bgcolor=%23ffffff&amp;src=e5m14f28esqqtiacerh1imp5io%40group.calendar.google.com&amp;color=%235F6B02&amp;ctz=America%2FToronto"></iframe>
        </div>
    </div> <!--End calendar section-->

    <div class="row section-contact text-center" id=contact>
        <div class="container text-center">
            <h1 class="display-5 section-title-contact">Contact Us</h1>
            <hr class="section-title-divider-contact">
        </div>
        <div class="container-fluid">
            <div class="row sub-section-contact">
                <div class="col-md-6 contact-entry"><i class="nav-icon fas fa-envelope"></i> <%= team.email %></div>
                <div class="col-md-6 contact-entry"><i class="nav-icon fas fa-envelope"></i>
                    <input class="text-center contact-entry" placeholder="Subscribe to mailing list">
                </div>
            </div>
        </div>
    </div> <!--End contact section-->

</main>

